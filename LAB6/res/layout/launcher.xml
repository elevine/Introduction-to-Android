			android:layout_height="wrap_content" android:text="Hello"
			android:padding="10dp" />
		<Button android:id="@+id/hello2_button" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Launch" />

	</LinearLayout>
	
		<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="horizontal">
		<TextView android:id="@+id/row3_tv" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="developer.android.com"
			android:padding="10dp" />
